#news_feed
  -@users.each do |user|
    = image_tag user.profile.image_1
    = user.name
    = user.gender.title
    = user.location
    / = user.age
    #message
      #message-button Message
      #message-box
        = form_tag create_message_path(recipient_id: user.id) do
          = fields_for :message do |i|
            %br
            = i.text_area :content, cols: '50', rows: '5'
          %br
          = submit_tag "Send", class: 'btn btn-success'

    %br



/ .popular tags

/ .local tech events